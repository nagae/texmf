% 除算の行内表現
\newcommand{\lfrac}[2]{ \left.{#1}\right/{#2} }
% 偏微分
\newcommand{\PDF}[2]{\frac{\partial #1}{\partial #2}}
\newcommand{\PDDF}[3]{\frac{\partial^2 #1}{\partial #2 \partial #3}}
\newcommand{\PDDDF}[4]{\frac{\partial^3 #1}{\partial #2 \partial #3 \partial #4}}
\newcommand{\PDNF}[3]{\frac{\partial^#3 #1}{\partial {#2}^#3}}
\newcommand{\ODF}[2]{\frac{\mathrm{d} #1}{\mathrm{d}#2}}
\newcommand{\ODNF}[3]{\frac{\mathrm{d}^#3 #1}{\mathrm{d} {#2}^#3}}
