% subject to
\newcommand{\Subto}[1]{\text{s.t. }{#1}}
% 行列の転置
%\newcommand{\Trns}[1]{{}'}
\newcommand{\Trns}[1]{#1^{\top}}
% 指数
\newcommand{\EXP}[1]{\mathrm{exp}\left[#1\right]}
% 期待値
\newcommand{\Expct}[1]{\mathbb{E}\left[#1\right]}
% indicator function
\newcommand{\Ind}[1]{I_{\left\{#1\right\}}}
